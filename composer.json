{
    "name": "batyukovstudio/apiato-swagger-generator",
    "description": "Swagger docs generator for Apiato",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "batyukovstudio.com",
            "email": "batyukovstudio@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Batyukovstudio\\ApiatoSwaggerGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Batyukovstudio\\ApiatoSwaggerGenerator\\SwaggerServiceProvider"
            ]
        }
    }
},
